<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.test.mappers.weeklyboardMapper">
	<insert id="insertBoard">
		{call SP_SET_TBWEEKLYREPORT(#{STARTWEEK},#{ENDWEEK},#{JOBGRADE},#{USERNM},#{CONTENT},#{PS},#{REMARK})}
	</insert>
	<select id="weeklyList" resultType="weeklyDTO">
		{call SP_GET_TBWEEKLYREPORT(#{STARTWEEK},#{ENDWEEK},#{USERNM})}
	</select>

</mapper>